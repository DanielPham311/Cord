{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "lib": ["dom", "ESNext"],
      "jsx": "react",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noEmit": true
    },
    "include": ["src"]
  }
  